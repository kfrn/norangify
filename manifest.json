{
"manifest_version": 2,
    "name": "norangify",
    "description": "Promoting the norange revolution",
    "version": "1.0",
    "icons": {
      "16": "images/norange_16.jpg",
      "48": "images/norange_48.jpg",
      "128": "images/norange_128.jpg"
    },
    "content_scripts": [
        {
          "matches": ["*://*/*"],
          "js": ["content.js"],
          "run_at": "document_end"
        }
    ]
}
